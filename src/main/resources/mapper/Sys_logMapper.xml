        <result column="response" property="response" />
        <result column="endTime" property="endTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, username, url, method, ip, class_method, args, runTime, response, endTime
    </sql>

</mapper>
